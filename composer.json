{
    "name": "ziumper/templodocs",
    "description": "A sacred tool to generate docs",
    "type": "library",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Ziumper\\Templodocs\\": "src/",
            "Ziumper\\Templodocs\\Tests\\Utils\\": "tests/utils"
        },
        "exclude-from-classmap": ["tests"]
    },
    "autoload-dev": {
        "psr-4": {
            "Ziumper\\Templodocs\\Tests\\Unit\\": "tests/unit",
            "Ziumper\\Templodocs\\Tests\\Integration\\": "tests/integration"
        }
    },
    "bin": [
        "bin/templodocs"
    ],
    "require": {
	"php": ">=8.3",
        "phpoffice/phpword": "^1.3",
        "league/csv": "^9.23",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-zip": "*",
        "ext-json": "*",
        "ext-xml": "*",
        "ext-filter": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.89",
        "squizlabs/php_codesniffer": "^4.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4"
    }
}
